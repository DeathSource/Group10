<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.client.principle.PrincipleViewInfoController">
   <children>
      <Label layoutX="301.0" layoutY="14.0" text="View general info" underline="true">
         <font>
            <Font name="System Bold Italic" size="36.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" layoutX="300.0" layoutY="206.0" spacing="15.0">
         <children>
            <Button fx:id="sbViewUsersBtn" mnemonicParsing="false" onAction="#btnPressViewUsers" prefWidth="300.0" text="VIEW USERS">
               <font>
                  <Font name="System Bold" size="26.0" />
               </font>
            </Button>
            <Button fx:id="sbViewExamsBtn" mnemonicParsing="false" onAction="#btnPressViewExams" prefWidth="300.0" text="VIEW EXAMS">
               <font>
                  <Font name="System Bold" size="26.0" />
               </font>
            </Button>
            <Button fx:id="sbViewQuestionsBtn" mnemonicParsing="false" onAction="#btnPressViewQuestions" prefWidth="300.0" text="VIEW QUESTIONS">
               <font>
                  <Font name="System Bold" size="26.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
