<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxHeight="400.0" maxWidth="750.0" minHeight="400.0" minWidth="750.0" prefHeight="400.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.client.teacher.TeacherMenuController">
   <children>
      <Hyperlink fx:id="sbTeacherNameLnk" onAction="#LnkTeacherName" prefHeight="23.0" prefWidth="107.0" text="TeacheName">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Hyperlink>
      <Hyperlink fx:id="sbLogoutLnk" onAction="#LnkLogout" prefHeight="23.0" prefWidth="76.0" text="Logout">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Hyperlink>
      <Label alignment="CENTER" prefHeight="45.0" prefWidth="300.0" text="Welcome, Teacher!" textAlignment="JUSTIFY">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets bottom="10.0" left="225.0" />
         </VBox.margin>
         <font>
            <Font size="31.0" />
         </font>
      </Label>
      <Button fx:id="sbStartExamBtn" alignment="TOP_CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#btnStartExam" prefHeight="25.0" prefWidth="150.0" text="Start Exam" textAlignment="JUSTIFY" underline="true">
         <VBox.margin>
            <Insets bottom="10.0" left="300.0" top="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="sbCreateQuestionBtn" contentDisplay="CENTER" mnemonicParsing="false" onAction="#btnCreateQuestion" prefHeight="25.0" prefWidth="150.0" text="Create Question" underline="true">
         <VBox.margin>
            <Insets bottom="10.0" left="300.0" top="5.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="sbEditQuestionBtn" contentDisplay="CENTER" mnemonicParsing="false" onAction="#btnEditQuestion" prefHeight="25.0" prefWidth="150.0" text="Edit Question" underline="true">
         <VBox.margin>
            <Insets bottom="10.0" left="300.0" top="5.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="sbCrreateExamBtn" contentDisplay="CENTER" mnemonicParsing="false" onAction="#btnCrreateExam" prefHeight="25.0" prefWidth="150.0" text="Crreate Exam" underline="true">
         <VBox.margin>
            <Insets bottom="10.0" left="300.0" top="5.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="sbEditExamBtn" contentDisplay="CENTER" mnemonicParsing="false" onAction="#btnEditExam" prefHeight="25.0" prefWidth="150.0" text="Edit Exam" underline="true">
         <VBox.margin>
            <Insets bottom="10.0" left="300.0" top="5.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="sbViewReportBtn" contentDisplay="CENTER" mnemonicParsing="false" onAction="#btnViewReport" prefHeight="25.0" prefWidth="150.0" text="View Report" underline="true">
         <VBox.margin>
            <Insets bottom="10.0" left="300.0" top="5.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="sbSettingsBtn" contentDisplay="CENTER" mnemonicParsing="false" onAction="#btnSettings" prefHeight="25.0" prefWidth="150.0" text="Settings" underline="true">
         <VBox.margin>
            <Insets bottom="10.0" left="300.0" top="5.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
