<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.client.teacher.TeacherCheckComputerizedResultsController">
   <children>
      <Label layoutX="268.0" layoutY="14.0" text="Computerized Results" underline="true">
         <font>
            <Font name="System Bold Italic" size="36.0" />
         </font>
      </Label>
      <TableView fx:id="sbComputerizedResultsTv" layoutX="110.0" layoutY="116.0" prefHeight="315.0" prefWidth="681.0" style="-fx-font-size: 15;">
        <columns>
          <TableColumn fx:id="sbExamIDTc" prefWidth="140.0" resizable="false" style="-fx-alignment: CENTER;" text="Exam ID" />
          <TableColumn fx:id="sbStudentIDTc" prefWidth="160.0" resizable="false" style="-fx-alignment: CENTER;" text="Student ID" />
            <TableColumn fx:id="sbComputerizedGrade" prefWidth="180.0" resizable="false" style="-fx-alignment: CENTER;" text="Computerized Grade" />
            <TableColumn fx:id="sbTeacherGradeTc" prefWidth="175.0" resizable="false" style="-fx-alignment: CENTER;" text="Final Grade" />
        </columns>
      </TableView>
      <HBox alignment="CENTER" layoutX="59.0" layoutY="457.0" spacing="5.0">
         <children>
            <Button fx:id="sdCheckAnswersBtn" mnemonicParsing="false" onAction="#btnPressCheckAnswers" text="Check Selected Answers">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <Label text="&gt;&gt;">
               <font>
                  <Font name="System Bold Italic" size="36.0" />
               </font>
            </Label>
            <Button fx:id="sbChangeGradeBtn" mnemonicParsing="false" onAction="#btnPressChangeGrade" text="Change Selected Grade">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <Label text="&gt;&gt;">
               <font>
                  <Font name="System Bold Italic" size="36.0" />
               </font>
            </Label>
            <Button fx:id="sbAcceptSelected" mnemonicParsing="false" onAction="#btnPressAcceptSelected" text="Accept Selected Grade">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
