            <Label layoutX="130.0" layoutY="3.0" text="choose exam bank :" underline="true">
               <font>
                  <Font name="System Italic" size="15.0" />
               </font>
            </Label>
            <ComboBox fx:id="sbChooseBankCb" layoutX="280.0" layoutY="2.0" onAction="#cbPressChooseBank" prefHeight="25.0" prefWidth="118.0" promptText="choose bank" />
