            <TableView fx:id="sbExamsTv" layoutX="130.0" layoutY="31.0" prefHeight="170.0" prefWidth="150.0">
              <columns>
                <TableColumn fx:id="sbExamIDTc" prefWidth="156.0" text="Exam ID" />
              </columns>
            </TableView>
            <Label layoutX="132.0" layoutY="4.0" text="select an exam to edit :" underline="true">
               <font>
                  <Font name="System Italic" size="15.0" />
