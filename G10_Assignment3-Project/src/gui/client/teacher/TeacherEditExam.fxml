<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.client.teacher.TeacherEditExamController">
   <children>
      <AnchorPane fx:id="sbBotPanelAp" layoutX="36.0" layoutY="156.0" prefHeight="230.0" prefWidth="528.0">
