            <Label layoutX="130.0" layoutY="3.0" text="choose exam course :" underline="true">
               <font>
                  <Font name="System Italic" size="15.0" />
               </font>
            </Label>
            <ComboBox fx:id="sbChooseCourseCb" layoutX="280.0" layoutY="2.0" onAction="#cbPressChooseCourse" promptText="choose course" />
            <Button fx:id="sbContinue2Btn" layoutX="433.0" layoutY="2.0" mnemonicParsing="false" onAction="#btnPressContinue2" text="continue&gt;&gt;&gt;" />
