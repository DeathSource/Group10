            <Button fx:id="sbEditSelectedExamBtn" layoutX="388.0" layoutY="204.0" mnemonicParsing="false" onAction="#btnPressEditSelectedExam" text="Edit selected exam&gt;&gt;&gt;" AnchorPane.rightAnchor="0.0" />
