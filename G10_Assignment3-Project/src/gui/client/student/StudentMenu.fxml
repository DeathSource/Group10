<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.client.student.StudentMenuController">
   <children>
      <HBox alignment="CENTER" layoutX="316.0" layoutY="14.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="75.0">
         <children>
            <Label fx:id="sbWelcomeLbl" alignment="CENTER" contentDisplay="CENTER" text="Welcome, Israel" textAlignment="JUSTIFY" textOverrun="CENTER_WORD_ELLIPSIS" underline="true">
               <font>
                  <Font name="System Bold Italic" size="36.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Hyperlink fx:id="sbLogoutLnk" layoutX="24.0" layoutY="51.0" onAction="#lnkPressLogout" text="Logout" underline="true" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="41.0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Hyperlink>
      <Hyperlink fx:id="sbStudentLnk" layoutX="24.0" layoutY="24.0" onAction="#btnPressSettings" text="StudentName" underline="true" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="14.0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Hyperlink>
      <HBox alignment="CENTER" layoutY="227.0" prefHeight="100.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <VBox alignment="CENTER" prefHeight="137.0" prefWidth="250.0" spacing="15.0">
               <children>
                  <Button fx:id="sbTakeExamBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnPressTakeExam" text="TAKE AN EXAM">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="sbViewExamResultsBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnPressViewExamResults" text="VIEW EXAM RESULTS">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="sbSettingsBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnPressSettings" text="SETTINGS">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
