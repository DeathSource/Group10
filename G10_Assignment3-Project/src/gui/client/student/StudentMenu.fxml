<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.client.student.StudentMenuController">
   <children>
      <Label fx:id="welcomeLbl" alignment="CENTER" contentDisplay="CENTER" layoutX="188.0" layoutY="14.0" text="Welcome, Israel" textAlignment="JUSTIFY" textOverrun="CENTER_WORD_ELLIPSIS" underline="true" AnchorPane.topAnchor="14.0">
         <font>
            <Font name="System Bold Italic" size="30.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" layoutX="213.0" layoutY="145.0" prefHeight="110.0" prefWidth="175.0" spacing="10.0">
         <children>
            <Button fx:id="sbTakeExamBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnPressTakeExam" text="Take an exam" />
            <Button fx:id="sbViewExamResultsBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnPressViewExamResults" text="View exam results" />
            <Button fx:id="sbSettingsBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnPressSettings" text="Settings" />
